@model Dialogue.Logic.Models.ViewModels.ViewTopicViewModel
@{
    var topic = Model.Topic;
}

@if (!Model.Permissions[AppConstants.PermissionDenyAccess].IsTicked)
{
    <div class="topicrowslim topicslimsticky-@topic.IsSticky topicslimlocked-@topic.IsLocked">

        <a href="@Html.Raw(topic.Url)">@Html.Raw(topic.Name)</a>

    </div>
}